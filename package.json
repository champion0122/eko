    "type": "git",
    "url": "git+https://github.com/FellouAI/eko.git"
  },
  "devDependencies": {
    "autoprefixer": "10.4.19",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "^5.8.3"
  }
